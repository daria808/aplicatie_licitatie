<Page x:Class="Client_ADBD.Views.AdminViewUserProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Client_ADBD.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminViewUserProfilePage">

    <Grid>
        <Button Grid.Row="7" Name="btnBack" Padding="5" Margin="0,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Command="{Binding BackCommand}" Background="#FF009688" BorderThickness="0" Height="40" Width="65">
            <Image Source="undo.png" Width="58" Height="36" />
        </Button>
    </Grid>
</Page>